    <depends href="http://schemas.ogf.org/occi/infrastructure#//@mixins[term='os_tpl']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins term="vmimage" scheme="http://occiware.org/occi/infrastructure/crtp/backend#">
    <attributes name="imagename" description="Name of the template if this compute has a template or will be created with a template.">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins term="credential" scheme="http://occiware.org/occi/infrastructure/crtp/backend#">
    <attributes name="user" required="true">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="password" required="true">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="occi.compute.userdata.file" required="false">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
  </mixins>
