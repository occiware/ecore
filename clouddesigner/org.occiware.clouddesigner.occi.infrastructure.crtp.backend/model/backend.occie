  <mixins term="vmaddon" scheme="http://occiware.org/occi/infrastructure/crtp/backend#" title="Addon attributes for virtual machine, specific to vmware" depends="//@mixins[term='vmimage']">
    <attributes name="vcpu">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="gueststate">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="guestosid">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="markedastemplate">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins term="linux" scheme="http://occiware.org/occi/infrastructure/crtp/backend#" title="Mixin os_tpl for linux systems">
    <attributes name="guestid" required="true" type="//@types.0"/>
    <attributes name="datastoreisopath">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <depends href="http://schemas.ogf.org/occi/infrastructure#//@mixins[term='os_tpl']"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins term="macosx" scheme="http://occiware.org/occi/infrastructure/crtp/backend#" title="Mixin os_tpl for mac os x systems">
